{
    "manifest_version": 2,
    "name": "Blacklister for pracc.com â™¥",
    "description": "Blacklist teams on pracc.com",
    "version": "1.0.0",
    "author": "https://github.com/marqdevx",
    "permissions": [
        "tabs",
        "cookies",
        "https://www.pracc.com/*"
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "marqdevx@github.com",
            "strict_min_version": "78.0"
        }
    },
    "content_scripts": [
        {
            "matches": [

                "*://*.pracc.com/*"
            ],
            "js": [
                "blacklister-pracc.js"
            ]
        }
    ]
}